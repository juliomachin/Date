package es.unileon.prg.tema7;


class Date{
	private int year;
	private int month;
	private int day;
	
	public Date(int day, int month, int year)
	{
		today = new Date("fecha");
		this.day = day;
		this.month = month;
		this.year = year;
	
		int getDay ()
		{
			return this.day;
		}
		void setDay(int Day)
		{
			this.day = day;
		
		}




	
	}
